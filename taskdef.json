{
  "family": "httpd-jenkins",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "volumesFrom": [],
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/var/log/httpd/",
          "sourceVolume": "httpd-logs",
          "readOnly": false
        }
      ],
      "name": "httpd",
      "image": "%REPOSITORY_URI%:httpd-v_%BUILD_NUMBER%",
      "cpu": 0,
      "memoryReservation": 128
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/tmp/"
      },
      "name": "httpd-logs"
    }
  ]
}
